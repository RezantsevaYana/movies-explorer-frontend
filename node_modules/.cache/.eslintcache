y(newShortList));\n        } else {\n            setShortList([]);\n            setMessage('Ничего не найдено');\n        }\n    }, [checkBoxStatus])\n\n\n    function handleCheckboxChange(isCheckboxOn) {\n        setChecked(isCheckboxOn);\n    }\n\n\n    return (\n        <section className=\"movies\">\n            <SearchForm setMoviesCount={setMoviesCount}\n                setMoviesList={setMoviesList}\n                handleSubmitSearchForm={handleSubmitSearchForm}\n                handleCheckboxChange={handleCheckboxChange}\n                checkBoxStatus={checkBoxStatus}\n                checked={checked}\n                setChecked={setChecked}\n                isLoading={isLoading}\n                setIsLoading={setIsLoading}\n            >\n            </SearchForm>\n            <MoviesCardList\n                setMoviesCount={setMoviesCount}\n                moviesCount={moviesCount}\n                moviesList={checkBoxStatus ? shortList : moviesList}\n                handleSaveMovies={handleSaveMovies}\n                handleDeleteMovies={handleDeleteMovies}\n                favoriteList={favoriteList}\n                addMovies={addMovies}\n                setAddMovies={setAddMovies}\n                message={message}\n                isLoading={isLoading}\n                setIsLoading={setIsLoading}\n            />\n            <Footer></Footer>\n        </section>\n\n\n    );\n}\n\nexport default Movies;\n","/Users/rezanceva96/movies-explorer-frontend/src/components/SearchForm/SearchForm.js",["202","203"],[],"/Users/rezanceva96/movies-explorer-frontend/src/components/MoviesCardList/MoviesCardList.js",["204","205"],[],"/Users/rezanceva96/movies-explorer-frontend/src/components/MoviesCard/MoviesCard.js",["206","207"],[],"import React, { useState } from \"react\";\nimport { Navlink, useLocation } from \"react-router-dom\";\n\nfunction MoviesCard({ movie, handleSaveMovies, handleDeleteMovies, favoriteList, favoriteListForRender, setFavoriteListForRender }